{
  "name": "@hypequery/core",
  "version": "0.1.0",
  "description": "Real-time ClickHouse query builder with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "generate-types": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node src/cli/bin.ts",
    "test-cli": "node --loader ts-node/esm src/cli/generate-test.ts",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@clickhouse/client-web": "^0.2.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.0.0",
    "jest": "^29.7.0",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.3"
  },
  "bin": {
    "hypequery-generate": "./dist/cli/bin.js"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifiers": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lureilly1/hypequery-core.git"
  },
  "homepage": "https://hypequery.dev",
  "bugs": {
    "url": "https://github.com/lureilly1/hypequery-core/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}