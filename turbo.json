{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": [
		"**/.env.*local"
	],
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"test": {
			"dependsOn": [
				"build"
			],
			"inputs": [
				"src/**/*.tsx",
				"src/**/*.ts",
				"test/**/*.ts",
				"test/**/*.tsx"
			]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"generate-types": {
			"cache": false,
			"dependsOn": [
				"build"
			]
		}
	}
}